{
  "name": "Тестовое расширение",
  "version": "0.0.1",
  "manifest_version": 2,
  "icons": {
    "128": "128.png"
  },
  "browser_action": {
    "default_title": "Тестовое расширение",
    "default_icon" : "128.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["collections.js", "functions.js", "background.js"],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "notifications",
    "alarms",
    "storage",
    "http://*/",
    "https://*/*"
  ]
}